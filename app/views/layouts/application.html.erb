<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <%= csrf_meta_tag %>
    <%= stylesheet_link_tag :all %>
    <%= javascript_include_tag :all %>
    <%= render 'layouts/stylesheets' %>
    <%= yield :head %>
  </head>
  <body onload="initialize()">
    <div class="container">
      <%= render 'layouts/header' %>
      
      <section class="round">
      
        <% flash.each do |key, value| %>
          <div class="flash <%= key %>"><%= value %></div>
        <% end %>
        
        <%= yield  %>
        
      </section>
      

      <%= yield :map %>
      
      
      <%= render 'layouts/footer' %>
    </div>
    

<br />

<br />
  </body>
</html>
